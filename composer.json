{
    "name": "twokings/likes",
    "description": "Add a like (üëç or ‚ù§) button on articles for users to leave a simple like",
    "type": "bolt-extension",
    "keywords": [
        "likes",
        "thumbs up",
        "upboat",
        "upvotes"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Bob den Otter",
            "email": "bob@twokings.nl"
        },
        {
            "name": "Xiao-Hu Tai",
            "email": "xiao@twokings.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\TwoKings\\Likes\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\TwoKings\\Likes\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\TwoKings\\Likes\\LikesExtension"
    }
}
